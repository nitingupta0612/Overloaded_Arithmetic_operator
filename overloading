#include<iostream>
#include<conio.h>
using namespace std;
class FLOAT1
{
	float a;
	public:
		FLOAT1(float a)
		{
			this->a=a;
		}
		float operator+(FLOAT1 F)
		{
			return(a+F.a);
		}
		float operator-(FLOAT1 F)
		{
			return(a-F.a);
		}
		float operator*(FLOAT1 F)
		{
			return(a*F.a);
		}
		float operator/(FLOAT1 F)
		{
			return(a/F.a);
		}
		  		
};
int main()
{
	FLOAT1 f1(28.12),f2(29.12);
	cout<<"\n\n Operator + : "<<f1+f2;
	cout<<"\n\n Operator - : "<<f1-f2;
	cout<<"\n\n Operator * : "<<f1*f2;
	cout<<"\n\n Operator / : "<<f1/f2;
return 0;
}
