#include<iostream>
#include<conio.h>
#include<math.h>
using namespace std;
class FLOAT
{
	float a;
	public:
		FLOAT(float a)
		{
			this->a=a;
		}
		float operator+(FLOAT F)
		{
			return(a+F.a);
		}
		float operator-(FLOAT F)
		{
			return(a-F.a);
		}
		float operator*(FLOAT F)
		{
			return(a*F.a);
		}
		float operator/(FLOAT F)
		{
			return(a/F.a);
		}
		  		
};
int main()
{
	FLOAT num1(23.12),num2(20.12);
	cout<<"\n\n Overloading Operator + : "<<num1+num2;
	cout<<"\n\n Overloading Operator - : "<<num1-num2;
	cout<<"\n\n  OverloadingOperator * : "<<num1*num2;
	cout<<"\n\n  OverloadingOperator / : "<<num1/num2;
return 0;
}
