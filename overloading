#include<iostream>
#include<conio.h>
#include<math.h>
using namespace std;
class FLOAT
{
	float a;
	public:
		FLOAT(float a)
		{
			this->a=a;
		}
		float operator+(FLOAT F)
		{
			return(a+F.a);
		}
		float operator-(FLOAT F)
		{
			return(a-F.a);
		}
		float operator*(FLOAT F)
		{
			return(a*F.a);
		}
		float operator/(FLOAT F)
		{
			return(a/F.a);
		}
		  		
};
int main()
{
	FLOAT F1(23.12),F2(20.12);
	cout<<"\n\n Operator + : "<<F1+F2;
	cout<<"\n\n Operator - : "<<F1-F2;
	cout<<"\n\n Operator * : "<<F1*F2;
	cout<<"\n\n Operator / : "<<F1/F2;
return 0;
}
